  "version": "0.2.0",
  "configurations": [
  {
    "name": "Attach by Process ID",
    "processId": "${command:PickProcess}",
    "request": "attach",
    "skipFiles": [
      "<node_internals>/**"
    ],
    "type": "node"
  },
  {
    "name": "Launch run",
    "program": "${workspaceFolder}/scripts/run.js",
    "request": "launch",
    "skipFiles": [
      "<node_internals>/**"
    ],
    "type": "node"
  },
    {
      "type": "pwa-node",
      "request": "launch",
      "name": "Launch profileQ",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "program": "${workspaceFolder}/test/profileQ.js",
      "runtimeExecutable": "/root/.nvm/versions/node/v16.3.0/bin/node"
    }
  ]
}